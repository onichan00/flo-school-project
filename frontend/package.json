{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 4040",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "npx http-server -p ${PORT} ./dist",
    "test:unit": "vue-cli-service test:unit",
    "test:clear-cache": "node_modules/.bin/jest --clearCache"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@headlessui/vue": "^1.7.3",
    "@heroicons/vue": "^2.0.11",
    "@iconify/vue": "^4.0.0",
    "@popperjs/core": "^2.11.6",
    "animate.css": "^4.1.1",
    "axios": "^1.2.0",
    "chart.js": "^3.9.1",
    "chartist": "^1.3.0",
    "core-js": "^3.8.3",
    "dateformat": "^5.0.3",
    "flowbite": "^1.5.3",
    "flowbite-vue": "^0.0.6",
    "http-server": "^14.1.1",
    "v-calendar": "^3.0.0-alpha.8",
    "vue": "^3.2.13",
    "vue-chartist": "^3.0.0",
    "vue-chartjs": "^4.1.2",
    "vue-datepicker-next": "^1.0.2",
    "vue-final-modal": "^3.4.11",
    "vue-router": "^4.1.5",
    "vue-toastification": "^2.0.0-rc.5",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/preset-env": "^7.20.2",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/vue3-jest": "^27.0.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.5.1",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.3",
    "vue-cli-plugin-tailwind": "~3.0.0",
    "vue-cli-plugin-vuetify": "^2.5.8"
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "testMatch": [
      "**/src/**/*.spec.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^[^.]+.vue$": "@vue/vue3-jest",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(dateformat|vue-datepicker-next|v-calendar|axios|flowbite|flowbite-vue|chartist))"
    ],
    "verbose": true
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "overrides": [
      {
        "files": [
          "**/src/**/*.spec.js"
        ],
        "rules": {
          "jest/expect-expect": "off"
        }
      }
    ],
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
